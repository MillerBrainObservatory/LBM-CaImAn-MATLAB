#!/bin/bash

INCLUDE_DIR=../external/ScanImageTiffReader-1.3-Linux/include
LIB_DIR=../external/ScanImageTiffReader-1.3-Linux/lib
LIB_NAME=ScanImageTiffReaderAPIStatic

mex -I$INCLUDE_DIR -L$LIB_DIR -l$LIB_NAME mexScanImageTiffClose.c
mex -I$INCLUDE_DIR -L$LIB_DIR -l$LIB_NAME mexScanImageTiffData.c
mex -I$INCLUDE_DIR -L$LIB_DIR -l$LIB_NAME mexScanImageTiffImageDescriptions.c
mex -I$INCLUDE_DIR -L$LIB_DIR -l$LIB_NAME mexScanImageTiffMetadata.c
mex -I$INCLUDE_DIR -L$LIB_DIR -l$LIB_NAME mexScanImageTiffOpen.c
mex -I$INCLUDE_DIR -L$LIB_DIR -l$LIB_NAME mexScanImageTiffReaderAPIVersion.c

